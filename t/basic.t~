use Test::More tests => 1;
require File::Temp;

my $file = File::Temp::tmpnam();
END { 1 while unlink $file };

system
    $^X,
        '-Mblib',
        '-MDevel::ContinuousProfiler',
        '-MData::Dumper',
        '-e' => '1 for 1 .. 1_000_000; print STDERR Data::Dumper::Dumper(\%Devel::ContinuousProfiler::DATA)';
is( $?, 0 );

